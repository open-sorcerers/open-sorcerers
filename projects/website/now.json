{
  "name": "open-sorcerers",
  "version": 2,
  "env": {
    "ENVIRONMENT": "staging"
  },
  "rewrites": [
    { "source": "/settings/:setting", "destination": "/settings" },
    { "source": "/ask/:setting", "destination": "/ask" },
    { "source": "/build/:setting", "destination": "/build" },
    { "source": "/learn/:setting", "destination": "/learn" },
    { "source": "/:whatever", "destination": "/404" }
  ]
}
